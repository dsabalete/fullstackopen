{
  "name": "blog-list",
  "version": "1.0.0",
  "description": "Blog list app - part 4",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "build:ui": "rm -rf build && cd ../../part2/notes && npm run build --prod && cp -r build ../../part4/notes-backend",
    "deploy": "git push heroku HEAD:master",
    "deploy:full": "npm run build:ui && git add . && git commit -m uibuild && npm run deploy",
    "logs:prod": "heroku logs --tail",
    "test": "jest --verbose",
    "lint": "eslint ."
  },
  "author": "David Sabalete",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.12.0"
  },
  "devDependencies": {
    "eslint": "^7.21.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7"
  }
}
